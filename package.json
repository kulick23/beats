{
  "name": "beats",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "dev": "vite",
    "build": "vite build --base=./",
    "serve": "vite --host=localhost --port=8080 ./ ",
    "preview": "vite preview",
    "lint": "eslint -c .eslintrc src/*.js",
    "lint:fix": "eslint -c .eslintrc src/*.js --fix",
    "prettier": "prettier --check . --ignore-unknown --ignore-path ../../config/.prettierignore",
    "prettier:fix": "prettier --write . --ignore-unknown --ignore-path ../../config/.prettierignore",
    "deploy": "vite build && git add dist && git commit -m 'Deploy to GitHub Pages' && git subtree push --prefix dist origin gh-pages"

  },
  "author": "Danila Kulinkovich",
  "license": "ISC",
  "devDependencies": {
    "vite": "^4.3.9"
  }
  
 
}
